class Factorial{
    public static void main(String[] a){
        System.out.println(new Fuc().ComputeFac(10));
    }// just a comment ignore it
}

class Fac {
    public int ComputeFac(int num) {
        double num_aux;
        if (num < 1) {
            num_aux = 0;
        }
        else {
            num_aux = num * (this.ComputeFac(num - 1)) ;
        }
        return num_aux ;
    }
}