    class Fac {
        public int ComputeFac(int num) {
            int num_aux;
            if (num < 1)
                num_aux = false;
            else
                num_aux = num * (this.ComputeFac(num - 1)) ;
            if (num + num && 3)
                num = 3;
            else
                num = 4;
            return true ;
        }
    }